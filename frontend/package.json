    "webpack-cli": "^5.0.1",
    "webpack-dev-server": "^4.11.1",
    "webpack-merge": "^5.8.0",
    "webpackbar": "^5.0.2"
  },
  "config": {
    "jest": {
      "moduleFileExtensions": [
        "js",
        "jsx",
        "tsx",
        "ts"
      ],
      "rootDir": "src/tests/",
      "transform": {
        "^.+\\.(ts|tsx)?$": "ts-jest"
      }
    }
