{
  "name": "seedlist-dashboard",
  "description": "Internal dashboard for managing the seedlist.",
  "private": true,
  "scripts": {
    "postinstall": "patch-package",
    "start": "cross-env NODE_ENV=development webpack serve",
    "build": "cross-env NODE_ENV=production webpack build",
    "analyze-bundle": "cross-env NODE_ENV=production ANALYZER=true webpack build",
    "test": "jest --passWithNoTests",
    "coverage": "jest --coverage",
    "type-check": "tsc --noEmit",
    "lint": "npm run eslint && npm run stylelint",
    "lint:fix": "npm run eslint:fix && npm run stylelint:fix",
    "eslint": "eslint \"src/**/*.{js,ts,jsx,tsx}\"",
    "eslint:fix": "eslint --fix \"src/**/*.{js,jsx,ts,tsx}\"",
    "stylelint": "stylelint \"src/**/*.{css,sass,scss,less}\"",
    "stylelint:fix": "stylelint --fix \"src/**/*.{css,sass,scss,less}\"",
    "prettier": "prettier --check \"(src|public)/**/*.{html,js,jsx,ts,tsx,css,sass,scss,less,json}\"",
    "prettier:fix": "prettier --write \"(src|public)/**/*.{html,js,jsx,ts,tsx,css,sass,scss,less,json}\"",
    "commit": "cz",
    "release": "standard-version"
  },
  "dependencies": {
