    "@mui/lab": "^5.0.0-alpha.118",
    "@mui/material": "^5.11.4",
    "@mui/x-data-grid": "^5.17.22",
    "axios": "^1.2.1",
    "graphql": "^16.6.0",
    "moment": "^2.29.4",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.42.1",
    "react-lodash": "^0.1.2",
    "react-moment": "^1.1.2",
    "react-router-dom": "^6.7.0"
  },
